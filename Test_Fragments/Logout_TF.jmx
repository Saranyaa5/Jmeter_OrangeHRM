      <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Pacing">
        <stringProp name="scriptLanguage">groovy</stringProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="script">int currentIteration = vars.get(&quot;pacingCounter&quot;)?.toInteger() ?: 1
int totalIterations = 2  // Match your Loop Controller&apos;s loop count

if (currentIteration &lt; totalIterations) {
    long pacingTime = 30000
    long startTime = vars.get(&quot;iterationStart&quot;)?.toLong() ?: System.currentTimeMillis()
    long now = System.currentTimeMillis()
    long elapsed = now - startTime
    long waitTime = pacingTime - elapsed

    if (waitTime &gt; 0) {
        log.info(&quot;Pacing active - waiting &quot; + waitTime + &quot; ms&quot;)
        Thread.sleep(waitTime)
    } else {
        log.info(&quot;Pacing not needed - already exceeded pacing time&quot;)
    }
} else {
    log.info(&quot;Last iteration - skipping wait&quot;)
}</stringProp>
