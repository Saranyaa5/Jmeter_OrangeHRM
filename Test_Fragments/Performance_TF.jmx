            <stringProp name="script">// Get the count of extracted IDs
int count = Integer.parseInt(vars.get(&quot;randtrackerId_matchNr&quot;))

// Pick a random index (1-based because JMeter arrays start at 1)
int randIndex = 1 + new Random().nextInt(count)

// Extract random tracker ID and firstName using the random index
def randomId = vars.get(&quot;randtrackerId_&quot; + randIndex)
def randomName = vars.get(&quot;trackerempFirstname_&quot; + randIndex)  // firstName is second JSON path

// Store them as independent variables for reuse
vars.put(&quot;randomTrackerId&quot;, randomId)
vars.put(&quot;randomFirstName&quot;, randomName)

// Optional log
log.info(&quot;Selected ID: ${randomId}, First Name: ${randomName}&quot;)
</stringProp>
